    {
        "id": "750d9501d77f53a2",
        "type": "function",
        "z": "fd8bb986c5cff482",
        "name": "function 1",
        "func": "// Retrieve current states of relay1 and relay2 from context or default to 0\nvar relay1 = context.get('relay1') || 0;\nvar relay2 = context.get('relay2') || 0;\n\nnode.warn(\"Incoming topic: \" + msg.topic);\nnode.warn(\"Incoming payload: \" + msg.payload);\n\nif (msg.topic === \"relay1\") {\n    relay1 = parseInt(msg.payload);  // Ensure payload is an integer\n    context.set('relay1', relay1);    // Store the state in context\n}\n\nif (msg.topic === \"relay2\") {\n    relay2 = parseInt(msg.payload);  // Ensure payload is an integer\n    context.set('relay2', relay2);   // Store the state in context\n}\n\n\nvar payload = {\n    \"relay\": {\n        \"relay1\": relay1,\n        \"relay2\": relay2\n    }\n};\nmsg.payload = JSON.stringify(payload);\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
