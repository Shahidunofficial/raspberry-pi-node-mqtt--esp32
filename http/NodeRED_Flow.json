        "id": "9912295941c4e082",
        "type": "function",
        "z": "d696bb610b1e8da8",
        "name": "function 2",
        "func": "// Initialize the relay data object\nvar relayData = {\n    relay: {\n        relay1: 0, // Default value\n        relay2: 0  // Default value\n    }\n};\n\n// Check which relay was triggered and update its value\nif (msg.topic === 'relay1') {\n    relayData.relay.relay1 = msg.payload;  // Set relay1 state based on button press\n}\nif (msg.topic === 'relay2') {\n    relayData.relay.relay2 = msg.payload;  // Set relay2 state based on button press\n}\n\n// Set the payload for the HTTP POST request\nmsg.payload = relayData;\n\n// Return the modified message\nreturn msg;\n",
