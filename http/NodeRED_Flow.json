    {
        "id": "8bda20236de8763a",
        "type": "function",
        "z": "d696bb610b1e8da8",
        "name": "recieve and extract the data",
        "func": "// Check if the 'sensor' object exists in the payload\nif (msg.payload.sensor) {\n    var humidity = msg.payload.sensor.humidity;\n    var temperature = msg.payload.sensor.temperature;\n\n    // Create two separate messages for the gauges\n    var tempMsg = { payload: temperature };\n    var humMsg = { payload: humidity };\n\n    // Create the response message for the HTTP Response node\n    var respMsg = { payload: \"Data received and processed successfully.\", res: msg.res };\n\n    // Return messages: [Temperature Gauge, Humidity Gauge, HTTP Response]\n    return [tempMsg, humMsg, respMsg];\n} else {\n    // Log an error and send an error response if 'sensor' data is missing\n    node.error(\"Sensor data not found in payload\", msg);\n\n    // Error response message for the HTTP Response node\n    var errorResp = { payload: \"Error: Sensor data not found.\", res: msg.res };\n\n    // Return null for the gauges and send the error response to the HTTP Response node\n    return [null, null, errorResp];\n}\n",
